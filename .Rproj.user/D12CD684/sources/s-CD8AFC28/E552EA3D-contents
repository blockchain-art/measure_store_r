\name{write_indexes_to_redis}
\alias{write_indexes_to_redis}
\title{Writes given indexes to RedisRoaring under a uniquely generated key}
\usage{
write_indexes_to_redis(indexes, roaring)
}
\arguments{
\item{indexes}{character variable e.g.: \code{c("1, 3, 4", "2")}}

\item{roaring}{RedisRoaring client e.g.: \cr
\code{confl <- redis_config(url='redis://127.0.0.1:6380/1')} \cr
\code{roaring <- redux::hiredis(confl)}}
}
\description{
Writes given indexes to RedisRoaring under a uniquely generated key
}
